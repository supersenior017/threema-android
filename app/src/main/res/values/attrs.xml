<?xml version="1.0" encoding="utf-8"?>
<resources>
	<declare-styleable name="SectionHeaderView">
		<attr name="android:text"/>
	</declare-styleable>

	<declare-styleable name="CountBoxView">
		<attr name="textSizeOverride" format="dimension"/>
		<attr name="backgroundOverride" format="reference"/>
		<attr name="paddingOverride" format="dimension"/>
	</declare-styleable>

	<!-- cropimage -->
	<attr name="cropImageStyle" format="reference"/>
	<declare-styleable name="CropImageView">
		<attr name="highlightColor" format="reference|color"/>
		<attr name="showThirds" format="boolean"/>
		<attr name="showHandles">
			<enum name="changing" value="0"/>
			<enum name="always" value="1"/>
			<enum name="never" value="2"/>
		</attr>
	</declare-styleable>

	<!-- Conversation bubble font sizes -->
	<declare-styleable name="FontStyle">
		<attr name="font_small" format="dimension"/>
		<attr name="font_medium" format="dimension"/>
		<attr name="font_large" format="dimension"/>
		<attr name="font_status" format="dimension"/>
		<attr name="font_textentry" format="dimension"/>
	</declare-styleable>

	<declare-styleable name="StepPagerStrip">
		<attr name="previousColor" format="reference|color"/>
		<attr name="selectedColor" format="reference|color"/>
		<attr name="nextColor" format="reference|color"/>
	</declare-styleable>

	<declare-styleable name="CircularProgressBar">
		<attr name="progressbar_progress" format="integer" />
		<attr name="progressbar_max" format="integer" />
		<attr name="progressbar_color" format="color" />
		<attr name="background_color" format="color" />
		<attr name="progressbar_width" format="dimension" />
		<attr name="background_width" format="dimension" />
		<attr name="border_width" format="dimension" />
	</declare-styleable>

	<!-- themeable attributes -->
	<attr name="textColorPrimary" format="reference"/>
	<attr name="textColorSecondary" format="reference"/>
	<attr name="textColorTertiary" format="reference"/>

	<attr name="bubble_selected" format="reference|color"/>
	<attr name="bubble_pressed" format="reference|color"/>
	<attr name="bubble_status" format="reference|color"/>
	<attr name="bubble_recv" format="reference|color"/>
	<attr name="bubble_send" format="reference|color"/>
	<attr name="bubble_pressed_overlay" format="reference|color"/>
	<attr name="bubble_selected_overlay" format="reference|color"/>
	<attr name="date_separator_bg_color" format="reference|color"/>
	<attr name="bubble_thumbnail_placeholder_tint" format="reference|color"/>
	<attr name="bubble_thumbnail_placeholder_background" format="reference|color"/>

<!--	<attr name="bubble_date_separator" format="reference"/>
-->
	<attr name="unread_messages_bg_color" format="reference|color"/>
	<attr name="unread_messages_text_color" format="reference|color"/>

	<attr name="detail_highlight_bg_color" format="reference|color"/>
	<attr name="divider_color" format="reference|color"/>
	<attr name="background_primary" format="reference|color"/>
	<attr name="background_secondary" format="reference|color"/>
	<attr name="emoji_picker_background_top" format="reference|color"/>
	<attr name="emoji_picker_background_main" format="reference|color"/>
	<attr name="emoji_picker_divider" format="reference|color"/>
	<attr name="emoji_picker_top_divider" format="reference|color"/>
	<attr name="emoji_picker_hint" format="reference|color"/>
	<attr name="ballot_matrix_header_background" format="reference"/>
	<attr name="image_tint_default" format="reference|color"/>
	<attr name="mention_background" format="reference|color"/>
	<attr name="mention_background_inverted" format="reference|color"/>
	<attr name="mention_text_color" format="reference|color"/>
	<attr name="mention_text_color_inverted" format="reference|color"/>
	<attr name="markup_marker_color" format="reference|color"/>
	<attr name="compose_bubble" format="reference|color"/>
	<attr name="compose_edittext_bubble" format="reference"/>
	<attr name="compose_container" format="reference|color"/>
	<attr name="directory_header_background" format="reference|color"/>
	<attr name="cardview_background" format="reference|color"/>
	<attr name="gallery_attach_item_background" format="reference|color"/>
	<attr name="secondary_accent_color" format="reference|color"/>
	<attr name="whatsnew_wallpaper_drawable" format="reference"/>
	<attr name="text_color_first_line" format="reference|color"/>
	<attr name="text_color_second_line" format="reference|color"/>
	<attr name="text_color_first_line_bold" format="reference|color"/>
	<attr name="text_color_second_line_bold" format="reference|color"/>
	<attr name="text_color_third_line" format="reference|color"/>
	<attr name="background_identity_popup" format="reference|color"/>
	<attr name="multipane_divider_start_color" format="reference|color"/>
	<attr name="multipane_divider_end_color" format="reference|color"/>
	<attr name="popup_menu_background" format="reference|color"/>
	<attr name="settings_multipane_selection_bg" format="reference|color"/>
	<attr name="quickscroll_background" format="reference|color" />
	<attr name="accent_alpha" format="reference|color" />
	<attr name="cardview_divider_color" format="reference|color" />
	<attr name="control_panel_background" format="reference|color" />
	<attr name="background_openNotice" format="reference|color" />
	<attr name="text_color_openNotice" format="reference|color" />
	<attr name="bottomsheet_grapple_color" format="reference|color" />
	<attr name="attach_status_bar_color_expanded" format="reference|color" />
	<attr name="attach_status_bar_color_collapsed" format="reference|color" />
	<attr name="attach_button_background" format="reference|color"/>
	<attr name="material_toolbar_color" format="reference|color" />
	<attr name="circle_button_stroke_color" format="reference|color"/>

	<!-- Confirm device credentials screen -->
	<attr name="confirmDeviceCredentialsSideMargin" format="dimension"/>
	<attr name="confirmDeviceCredentialsTopMargin" format="dimension"/>

	<!-- wizard -->
	<declare-styleable name="NewWizardStepPagerStrip">
		<attr name="newWizardPreviousColor" format="reference|color"/>
		<attr name="newWizardSelectedColor" format="reference|color"/>
		<attr name="newWizardNextColor" format="reference|color"/>
	</declare-styleable>

	<!-- attachment sheet -->
	<declare-styleable name="CircleBackgroundImageView">
		<attr name="bgColor" format="color"/>
	</declare-styleable>

	<!--send button -->
	<declare-styleable name="SendButton">
		<attr name="buttonTheme" format="integer" />
	</declare-styleable>

	<!-- Camera View -->
	<attr name="scaleType" format="enum">
		<enum name="fillStart" value="0" />
		<enum name="fillCenter" value="1" />
		<enum name="fillEnd" value="2" />
		<enum name="fitStart" value="3" />
		<enum name="fitCenter" value="4" />
		<enum name="fitEnd" value="5" />
	</attr>

	<declare-styleable name="CameraView">
		<attr name="scaleType" />
		<attr name="lensFacing" format="enum">
			<enum name="none" value="0" />
			<enum name="front" value="1" />
			<enum name="back" value="2" />
		</attr>
		<attr name="captureMode" format="enum">
			<enum name="image" value="0" />
			<enum name="video" value="1" />
			<enum name="mixed" value="2" />
		</attr>
		<attr name="flash" format="enum">
			<enum name="auto" value="1" />
			<enum name="on" value="2" />
			<enum name="off" value="4" />
		</attr>
		<attr name="pinchToZoomEnabled" format="boolean" />
	</declare-styleable>

	<declare-styleable name="PreviewView">
		<attr name="scaleType" />
	</declare-styleable>

	<!-- Zoomable TextureView -->
	<declare-styleable name="ZoomableTextureView">
		<attr name="minScale" format="float" />
		<attr name="maxScale" format="float" />
	</declare-styleable>

	<!-- Button for Media Attacher Control Panel -->
	<declare-styleable name="ControlPanelButton">
		<attr name="labelIcon" format="integer"/>
		<attr name="labelText" format="integer"/>
		<attr name="strokeColor" format="color"/>
		<attr name="fillColor" format="color" />
		<attr name="foregroundColor" format="color" />
		<attr name="fillColorAlpha" format="integer" />
	</declare-styleable>

</resources>

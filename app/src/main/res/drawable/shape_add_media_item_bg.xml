<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item
		android:bottom="1dp"
		android:left="1dp"
		android:right="1dp"
		android:top="1dp">
		<shape android:shape="rectangle">
			<corners android:radius="@dimen/media_attach_button_radius" />
			<stroke
				android:width="2dp"
				android:color="@android:color/white"
				android:dashGap="2dp"
				android:dashWidth="2dp" />
		</shape>
	</item>
</layer-list>

